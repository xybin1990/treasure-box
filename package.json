{
  "name": "treasure-box",
  "version": "0.0.1",
  "main": "dist-electron/main/index.js",
  "description": "Treasure Box",
  "author": "",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build && electron-builder",
