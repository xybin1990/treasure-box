  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^3.1.2",
    "@vitejs/plugin-vue-jsx": "^2.1.0",
    "cross-env": "^7.0.3",
    "electron": "^21.1.0",
    "electron-builder": "^23.3.3",
    "typescript": "^4.8.4",
    "vite": "^3.1.4",
    "vite-electron-plugin": "^0.4.6",
    "vite-plugin-electron-renderer": "^0.10.1",
    "vue-tsc": "^0.40.13"
  },
  "dependencies": {
    "electron-updater": "^5.3.0",
    "vue": "^3.2.41",
    "vue-router": "^4.1.6"
  },
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344"
    }
  },
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "vue3",
    "vue"
